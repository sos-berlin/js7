#!/usr/bin/env bash
set -e
echo NOT WORKING
exit 1

cd "$(dirname "$0")/../bin/.."

(cd files/home/jobscheduler/agent && mkdir --parents config/private)
keystore="files/home/jobscheduler/agent/config/private/https.jks"
rm -f "$keystore"
keytool -genkey -alias test-agent-certificate -keyalg RSA -keysize 1024 -dname "CN=localhost, O=TEST" -validity 3660 -keypass "jobscheduler" -storepass "jobscheduler" -keystore "$keystore"
