#!/usr/bin/env bash
set -euo pipefail

cat logs/test.log \
  | grep -E -- "-Controller-.* - :| TEST | ERROR | WARN  | info  " \
  | grep -Ev "AgentEventsObserved|SubagentEventsObserved| info  .* js7.journal.Journal - Event " \
  | sed -E 's/(js7.journal.Journal - :).{32}(.*$)/\1 \2/'  \
  | sed -E 's/ \[[^]]*] / /' \
  | sed -E 's/ js7\.[^ ]* / /' \
  | cut -c 1-25,35- \
  | less
