#!/usr/bin/env bash
set -euo pipefail

a=()
if [[ $# -ge 1 ]]; then
  a+=("$1")
elif [[ -t 0 ]]; then
  a+=(logs/test.log)
fi

exec grep -E --line-buffered -- " ###| ERROR | WARN  | TEST - |-Controller-.* - :" "${a[@]}" |
  grep -Evw --line-buffered "AgentEventsObserved" |
  sed -l 's/ trace .*Journal - ://'
