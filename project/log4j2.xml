<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <console name="console" target="SYSTEM_ERR">
      <patternLayout pattern="%highlight{%d{EE HH:mm:ss.SSS} [%thread] %-5level %logger - %message}%n"/>
    </console>
  </appenders>
  <loggers>
    <root level="INFO">
      <appenderRef ref="console"/>
    </root>
  </loggers>
</configuration>
